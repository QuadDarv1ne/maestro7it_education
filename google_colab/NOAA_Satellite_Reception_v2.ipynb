    "# –ü–∞—Ä–∞–º–µ—Ç—Ä—ã –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é\n",
    "CONFIG = {\n",
    "    'sdr': {\n",
    "        'sample_rate': 2.4e6,\n",
    "        'center_freq': 137.62e6,\n",
    "        'gain': 40,\n",
    "        'ppm_correction': 0,\n",
    "    },\n",
    "    'decoder': {\n",
    "        'carrier_freq': 2400,\n",
    "        'line_rate': 2,\n",
    "        'pixels_per_line': 4160,\n",
    "        'audio_sample_rate': 20800,\n",
    "    },\n",
    "    'filters': {\n",
    "        'bandpass_low': 2300,\n",
    "        'bandpass_high': 2500,\n",
    "        'fir_taps': 101,\n",
    "    },\n",
    "    'image': {\n",
    "        'denoise_strength': 0.5,\n",
    "        'contrast_enhance': True,\n",
    "    },\n",
    "    'output': {\n",
    "        'directory': './noaa_output',\n",
    "    }\n",
    "}\n",
    "\n",
    "# –°–æ–∑–¥–∞–Ω–∏–µ –¥–∏—Ä–µ–∫—Ç–æ—Ä–∏–∏ –¥–ª—è –≤—ã—Ö–æ–¥–Ω—ã—Ö —Ñ–∞–π–ª–æ–≤\n",
    "output_dir = Path(CONFIG['output']['directory'])\n",
    "output_dir.mkdir(parents=True, exist_ok=True)\n",
    "print(f\"üìÅ –î–∏—Ä–µ–∫—Ç–æ—Ä–∏—è –≤—ã–≤–æ–¥–∞: {output_dir.absolute()}\")"
