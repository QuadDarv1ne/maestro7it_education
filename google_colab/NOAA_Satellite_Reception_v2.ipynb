   "source": [
    "class NOAADecoder:\n",
    "    \"\"\"\n",
    "    –£–ª—É—á—à–µ–Ω–Ω—ã–π –¥–µ–∫–æ–¥–µ—Ä NOAA APT —Å:\n",
    "    - FIR-—Ñ–∏–ª—å—Ç—Ä–∞–º–∏ —Å –æ–∫–Ω–æ–º –ö–∞–π–∑–µ—Ä–∞\n",
    "    - –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–π —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∫–æ–π —É—Å–∏–ª–µ–Ω–∏—è (AGC)\n",
    "    - –£–¥–∞–ª–µ–Ω–∏–µ–º —à—É–º–æ–≤\n",
    "    \"\"\"\n",
    "    \n",
    "    PIXELS_PER_LINE = 4160\n",
    "    LINE_RATE = 2  # —Å—Ç—Ä–æ–∫/—Å–µ–∫\n",
    "    PIXEL_RATE = PIXELS_PER_LINE * LINE_RATE  # 8320 –ø–∏–∫—Å–µ–ª–µ–π/—Å–µ–∫\n",
    "    \n",
    "    def __init__(self, config: dict = None):\n",
    "        self.config = config or CONFIG\n",
    "        self.sample_rate = self.config['sdr']['sample_rate']\n",
    "        self.carrier_freq = self.config['decoder']['carrier_freq']\n",
    "        self.audio_sample_rate = self.config['decoder']['audio_sample_rate']\n",
    "        \n",
    "    def simulate_iq_signal(self, duration: float) -> np.ndarray:\n",
    "        \"\"\"–°–∏–º—É–ª—è—Ü–∏—è IQ-—Å–∏–≥–Ω–∞–ª–∞ NOAA APT\"\"\"\n",
    "        t = np.arange(0, duration, 1/self.sample_rate)\n",
    "        \n",
    "        # –ú–æ–¥—É–ª–∏—Ä—É—é—â–∏–π —Å–∏–≥–Ω–∞–ª (\"–∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ\")\n",
    "        line_rate = self.config['decoder']['line_rate']\n",
    "        modulation = 0.5 * (1 + np.sin(2 * np.pi * line_rate * t))\n",
    "        modulation *= 0.8 + 0.2 * np.sin(2 * np.pi * 0.5 * t)\n",
    "        \n",
    "        # AM –º–æ–¥—É–ª—è—Ü–∏—è –Ω–∞ –ø–æ–¥–Ω–µ—Å—É—â–µ–π\n",
    "        carrier = np.cos(2 * np.pi * self.carrier_freq * t)\n",
    "        quadrature = np.sin(2 * np.pi * self.carrier_freq * t)\n",
    "        \n",
    "        i_channel = modulation * carrier\n",
    "        q_channel = modulation * quadrature\n",
    "        \n",
    "        # –î–æ–±–∞–≤–ª—è–µ–º —à—É–º\n",
    "        noise_level = 0.1\n",
    "        i_channel += noise_level * np.random.randn(len(t))\n",
    "        q_channel += noise_level * np.random.randn(len(t))\n",
    "        \n",
    "        return i_channel + 1j * q_channel\n",
    "    \n",
    "    def demodulate_iq(self, iq_data: np.ndarray) -> np.ndarray:\n",
    "        \"\"\"–î–µ–º–æ–¥—É–ª—è—Ü–∏—è IQ-–¥–∞–Ω–Ω—ã—Ö (–æ–≥–∏–±–∞—é—â–∞—è)\"\"\"\n",
    "        return np.abs(iq_data)\n",
    "    \n",
    "    def design_fir_bandpass(self, low: float, high: float, fs: float) -> np.ndarray:\n",
    "        \"\"\"FIR –ø–æ–ª–æ—Å–æ–≤–æ–π —Ñ–∏–ª—å—Ç—Ä —Å –æ–∫–Ω–æ–º –ö–∞–π–∑–µ—Ä–∞\"\"\"\n",
    "        nyq = fs / 2\n",
    "        taps = signal.firwin(\n",
    "            self.config['filters']['fir_taps'],\n",
    "            [low/nyq, high/nyq],\n",
    "            pass_zero=False,\n",
    "            window='kaiser',\n",
    "            scale=True\n",
    "        )\n",
    "        return taps\n",
    "    \n",
    "    def apply_agc(self, signal_data: np.ndarray, target: float = 0.5) -> np.ndarray:\n",
    "        \"\"\"–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∫–∞ —É—Å–∏–ª–µ–Ω–∏—è\"\"\"\n",
    "        # –û—Ü–µ–Ω–∫–∞ —É—Ä–æ–≤–Ω—è —Å–∏–≥–Ω–∞–ª–∞\n",
    "        kernel_size = max(3, int(len(signal_data) * 0.01))\n",
    "        if kernel_size % 2 == 0:\n",
    "            kernel_size += 1\n",
    "        \n",
    "        envelope = np.abs(signal_data)\n",
    "        smoothed = signal.medfilt(envelope, kernel_size=kernel_size)\n",
    "        smoothed = np.maximum(smoothed, 1e-10)\n",
    "        \n",
    "        # –ö–æ—ç—Ñ—Ñ–∏—Ü–∏–µ–Ω—Ç —É—Å–∏–ª–µ–Ω–∏—è\n",
    "        gain = np.clip(target / smoothed, 0.1, 10.0)\n",
    "        return signal_data * gain\n",
    "    \n",
    "    def decode_full(self, iq_data: np.ndarray) -> Tuple[np.ndarray, dict]:\n",
    "        \"\"\"–ü–æ–ª–Ω–æ–µ –¥–µ–∫–æ–¥–∏—Ä–æ–≤–∞–Ω–∏–µ IQ-–¥–∞–Ω–Ω—ã—Ö\"\"\"\n",
    "        metadata = {'iq_samples': len(iq_data)}\n",
    "        \n",
    "        # 1. –î–µ–º–æ–¥—É–ª—è—Ü–∏—è IQ\n",
    "        audio = self.demodulate_iq(iq_data)\n",
    "        \n",
    "        # 2. –î–µ—Ü–∏–º–∞—Ü–∏—è –¥–æ –∞—É–¥–∏–æ —á–∞—Å—Ç–æ—Ç—ã\n",
    "        decimation = int(self.sample_rate / self.audio_sample_rate)\n",
    "        audio_dec = signal.decimate(audio, decimation, ftype='fir')\n",
    "        metadata['audio_sample_rate'] = self.audio_sample_rate\n",
    "        \n",
    "        # 3. AGC\n",
    "        audio_agc = self.apply_agc(audio_dec)\n",
    "        \n",
    "        # 4. –ü–æ–ª–æ—Å–æ–≤–∞—è —Ñ–∏–ª—å—Ç—Ä–∞—Ü–∏—è (2400 –ì—Ü)\n",
    "        taps = self.design_fir_bandpass(\n",
    "            self.config['filters']['bandpass_low'],\n",
    "            self.config['filters']['bandpass_high'],\n",
    "            self.audio_sample_rate\n",
    "        )\n",
    "        filtered = signal.lfilter(taps, [1.0], audio_agc)\n",
    "        \n",
    "        # 5. –î–µ–º–æ–¥—É–ª—è—Ü–∏—è –æ–≥–∏–±–∞—é—â–µ–π\n",
    "        envelope = np.abs(signal.hilbert(filtered))\n",
    "        envelope = envelope - np.mean(envelope)\n",
    "        \n",
    "        # 6. –†–µ—Å–µ–º–ø–ª–∏–Ω–≥ –∫ —á–∞—Å—Ç–æ—Ç–µ –ø–∏–∫—Å–µ–ª–µ–π\n",
    "        target_samples = int(len(envelope) * self.PIXEL_RATE / self.audio_sample_rate)\n",
    "        pixels = signal.resample(envelope, target_samples)\n",
    "        \n",
    "        # 7. –§–æ—Ä–º–∏—Ä–æ–≤–∞–Ω–∏–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è\n",
    "        num_lines = len(pixels) // self.PIXELS_PER_LINE\n",
    "        trimmed = pixels[:num_lines * self.PIXELS_PER_LINE]\n",
    "        image_2d = trimmed.reshape(num_lines, self.PIXELS_PER_LINE)\n",
    "        \n",
    "        # –ù–æ—Ä–º–∞–ª–∏–∑–∞—Ü–∏—è\n",
    "        img_min, img_max = image_2d.min(), image_2d.max()\n",
    "        if img_max - img_min > 0:\n",
    "            image_norm = ((image_2d - img_min) / (img_max - img_min) * 255).astype(np.uint8)\n",
    "        else:\n",
    "            image_norm = np.zeros_like(image_2d)\n",
    "        \n",
    "        metadata['image_shape'] = image_norm.shape\n",
    "        metadata['lines'] = num_lines\n",
    "        \n",
    "        return image_norm, metadata\n",
    "    \n",
    "    def denoise(self, image: np.ndarray, strength: float = 0.5) -> np.ndarray:\n",
    "        \"\"\"–£–¥–∞–ª–µ–Ω–∏–µ —à—É–º–æ–≤ –º–µ–¥–∏–∞–Ω–Ω—ã–º —Ñ–∏–ª—å—Ç—Ä–æ–º\"\"\"\n",
    "        kernel = max(3, int(5 * strength))\n",
    "        if kernel % 2 == 0:\n",
    "            kernel += 1\n",
    "        return median_filter(image, size=kernel)\n",
    "    \n",
    "    def enhance_contrast(self, image: np.ndarray) -> np.ndarray:\n",
    "        \"\"\"–£–ª—É—á—à–µ–Ω–∏–µ –∫–æ–Ω—Ç—Ä–∞—Å—Ç–∞ —á–µ—Ä–µ–∑ –≥–∏—Å—Ç–æ–≥—Ä–∞–º–º—É\"\"\"\n",
    "        # –ü—Ä–æ—Å—Ç–æ–µ –≤—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –≥–∏—Å—Ç–æ–≥—Ä–∞–º–º—ã\n",
    "        flat = image.flatten()\n",
    "        sorted_indices = np.argsort(flat)\n",
    "        ranked = np.argsort(sorted_indices)\n",
    "        enhanced = (ranked / len(flat) * 255).astype(np.uint8)\n",
    "        return enhanced.reshape(image.shape)\n",
    "    \n",
    "    def save_image(self, image: np.ndarray, filepath: str, metadata: dict = None) -> str:\n",
    "        \"\"\"–°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è —Å –º–µ—Ç–∞–¥–∞–Ω–Ω—ã–º–∏\"\"\"\n",
    "        img = Image.fromarray(image, mode='L')\n",
    "        \n",
    "        # –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –º–µ—Ç–∞–¥–∞–Ω–Ω—ã—Ö\n",
    "        if metadata:\n",
    "            from PIL import PngImagePlugin\n",
    "            meta = PngImagePlugin.PngInfo()\n",
    "            for key, value in metadata.items():\n",
    "                meta.add_text(str(key), str(value))\n",
    "            img.save(filepath, 'PNG', pnginfo=meta)\n",
    "        else:\n",
    "            img.save(filepath, 'PNG')\n",
    "        \n",
    "        return filepath"
   ]
