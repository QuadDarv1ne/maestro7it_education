    "class SignalVisualizer:\n",
    "    \"\"\"–í–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏—è —Å–∏–≥–Ω–∞–ª–æ–≤ –∏ —Å–ø–µ–∫—Ç—Ä–æ–≤\"\"\"\n",
    "    \n",
    "    def __init__(self, dpi: int = 150, figsize: tuple = (12, 8)):\n",
    "        self.dpi = dpi\n",
    "        self.figsize = figsize\n",
    "    \n",
    "    def plot_spectrum(self, signal_data: np.ndarray, sample_rate: float, \n",
    "                     title: str = \"–°–ø–µ–∫—Ç—Ä\", output_file: str = None) -> plt.Figure:\n",
    "        \"\"\"–ü–æ—Å—Ç—Ä–æ–µ–Ω–∏–µ —Å–ø–µ–∫—Ç—Ä–∞ –º–æ—â–Ω–æ—Å—Ç–∏\"\"\"\n",
    "        fig, ax = plt.subplots(figsize=self.figsize, dpi=self.dpi)\n",
    "        \n",
    "        nfft = min(4096, len(signal_data))\n",
    "        fft_data = np.fft.fft(signal_data[:nfft])\n",
    "        freq = np.fft.fftfreq(nfft, 1/sample_rate)\n",
    "        \n",
    "        pos_mask = freq > 0\n",
    "        magnitude = np.abs(fft_data[pos_mask])\n",
    "        magnitude_db = 20 * np.log10(magnitude + 1e-10)\n",
    "        magnitude_db -= magnitude_db.max()\n",
    "        \n",
    "        ax.plot(freq[pos_mask] / 1000, magnitude_db, linewidth=0.5, color='red')\n",
    "        ax.set_xlabel(\"–ß–∞—Å—Ç–æ—Ç–∞ (–∫–ì—Ü)\")\n",
    "        ax.set_ylabel(\"–ú–æ—â–Ω–æ—Å—Ç—å (dB)\")\n",
    "        ax.set_title(title)\n",
    "        ax.grid(True, alpha=0.3)\n",
    "        ax.axvline(x=2.4, color='green', linestyle='--', alpha=0.7, label='2400 –ì—Ü')\n",
    "        ax.legend()\n",
    "        \n",
    "        if output_file:\n",
    "            fig.savefig(output_file, dpi=self.dpi, bbox_inches='tight')\n",
    "        \n",
    "        return fig\n",
    "    \n",
    "    def plot_waterfall(self, signal_data: np.ndarray, sample_rate: float,\n",
    "                      title: str = \"–°–ø–µ–∫—Ç—Ä–æ–≥—Ä–∞–º–º–∞\", output_file: str = None) -> plt.Figure:\n",
    "        \"\"\"–ü–æ—Å—Ç—Ä–æ–µ–Ω–∏–µ —Å–ø–µ–∫—Ç—Ä–æ–≥—Ä–∞–º–º—ã\"\"\"\n",
    "        fig, ax = plt.subplots(figsize=(self.figsize[0], self.figsize[1] * 1.2), dpi=self.dpi)\n",
    "        \n",
    "        nperseg = min(1024, len(signal_data) // 10)\n",
    "        f, t, Sxx = signal.spectrogram(signal_data, fs=sample_rate, \n",
    "                                       nperseg=nperseg, noverlap=nperseg//2)\n",
    "        \n",
    "        Sxx_db = 10 * np.log10(Sxx + 1e-10)\n",
    "        im = ax.pcolormesh(t, f/1000, Sxx_db, shading='gouraud', cmap='viridis')\n",
    "        \n",
    "        ax.set_xlabel(\"–í—Ä–µ–º—è (—Å–µ–∫)\")\n",
    "        ax.set_ylabel(\"–ß–∞—Å—Ç–æ—Ç–∞ (–∫–ì—Ü)\")\n",
    "        ax.set_title(title)\n",
    "        fig.colorbar(im, ax=ax, label='–ú–æ—â–Ω–æ—Å—Ç—å (dB)')\n",
    "        ax.axhline(y=2.4, color='red', linestyle='--', alpha=0.7, linewidth=2)\n",
    "        \n",
    "        if output_file:\n",
    "            fig.savefig(output_file, dpi=self.dpi, bbox_inches='tight')\n",
    "        \n",
    "        return fig\n",
    "    \n",
    "    def plot_all(self, iq_data: np.ndarray, sample_rate: float, \n",
    "                output_dir: str, prefix: str = \"\") -> list:\n",
    "        \"\"\"–°–æ–∑–¥–∞–Ω–∏–µ –≤—Å–µ—Ö –≤–∏–∑—É–∞–ª–∏–∑–∞—Ü–∏–π\"\"\"\n",
    "        output_path = Path(output_dir)\n",
    "        output_path.mkdir(parents=True, exist_ok=True)\n",
    "        \n",
    "        timestamp = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
    "        saved_files = []\n",
    "        \n",
    "        audio = np.abs(iq_data)\n",
    "        \n",
    "        # –°–ø–µ–∫—Ç—Ä\n",
    "        spectrum_file = output_path / f\"{prefix}spectrum.png\"\n",
    "        self.plot_spectrum(audio, sample_rate, \"–°–ø–µ–∫—Ç—Ä –º–æ—â–Ω–æ—Å—Ç–∏ NOAA APT\", \n",
    "                          output_file=str(spectrum_file))\n",
    "        saved_files.append(str(spectrum_file))\n",
